<%- include('./partials/header') %>
    <h1>Hello</h1>
    <h1>Gallery</h1>
    <%# JSON.stringify(galleryData) %>
        <div class="gallery">
            <% galleryData.forEach(picture=> { %>
                <figure>
                    <a href="/single/<%= picture.id %>">
                        <img src=<%=picture.url%> alt="">
                    </a>
                    <figcaption>
                        <p>
                            <%=picture.author%>
                        </p>
                        <p>
                            <%=picture.rating%>
                        </p>
                    </figcaption>
                </figure>
                <%})%>
        </div>
        <%- include('./partials/footer') %>